program test;
start
var int16_t var1, bool varb, string vars;
var1 := 10;
varb:=true;
vars:= "";
if((!!(var1 = 10)) = !!(var1 <> 10)) goto yes;
goto not;

yes start
    var;
    vars := "Var1 is equal 10";
    goto end;
    finish

no start
    var;
    vars := "Var1 isn't equal 10";
    goto end;
    finish

end;

for var1 to var1 + 100 next
start
    var;
    vars := "test for cycle";
    finish
finish